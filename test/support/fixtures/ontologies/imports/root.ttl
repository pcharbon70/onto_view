@prefix : <http://example.org/root#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/root#> a owl:Ontology ;
    rdfs:label "Root Ontology"@en ;
    rdfs:comment "Root ontology importing types"@en ;
    owl:imports <http://example.org/types#> .

:RootClass a owl:Class ;
    rdfs:label "Root Class"@en ;
    rdfs:comment "A class in the root ontology"@en .
